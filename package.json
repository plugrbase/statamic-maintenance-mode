    "laravel-mix": "^6.0.6",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "resolve-url-loader": "^4.0.0",
    "sass": "^1.45.0",
    "tailwindcss": "^2.2.19",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.12"
  },
